{
    "manifest_version": 3,
    "name": "Canvas Blackout",
    "description": "Blacks out Canvas' website",
    "version": "0.12.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://roseburg.instructure.com/*",
            "https://*.instructure.com/*"],

            "css": ["style.css"]
        }
    ],
    "action": {
        "default_popup": "index.html"
    }
}